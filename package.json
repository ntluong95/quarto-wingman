{
  "name": "quarto-wingman",
  "displayName": "Quarto Wingman",
  "description": "",
  "version": "0.0.2",
  "publisher": "ntluong95",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ntluong95/quarto-wingsman.git"
  },
  "icon": "resources/icon.png",
  "bugs": {
    "url": "https://github.com/ntluong95/quarto-wingsman/issues"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "python",
    "r",
    "quarto"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:r"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "grammars": [
      {
        "scopeName": "python.customInjection",
        "path": "./syntaxes/python-custom-injection.json",
        "injectTo": [
          "meta.embedded.block.python",
          "markup.fenced_code.block.markdown",
          "text.html.quarto",
          "text.html.markdown"
        ]
      },
      {
        "scopeName": "text.html.quarto.quartofn",
        "path": "./syntaxes/quartofn.tmLanguage.json",
        "injectTo": [
          "text.html.quarto"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "1.96.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "20.x",
    "eslint": "^9.21.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "glob": "^7.2.0",
    "mocha": "^10.8.2",
    "typescript": "^5.7.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {}
}
