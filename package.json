{
  "name": "quarto-wingman",
  "displayName": "Quarto Wingman",
  "description": "",
  "version": "0.0.4",
  "publisher": "ntluong95",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ntluong95/quarto-wingman.git"
  },
  "bugs": {
    "url": "https://github.com/ntluong95/quarto-wingman/issues"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "icon": "resources/icon.png",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "python",
    "r",
    "quarto"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:r",
    "onLanguage:quarto"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quarto-wingman.configureCellOptions",
        "title": "Quarto Wingman: Cell Options"
      },
      {
        "command": "inline-repl.run",
        "title": "Run Inline Code"
      }
    ],
    "grammars": [
      {
        "scopeName": "python.customInjection",
        "path": "./syntaxes/python-custom-injection.json",
        "injectTo": [
          "meta.embedded.block.python",
          "markup.fenced_code.block.markdown",
          "text.html.quarto",
          "text.html.markdown"
        ]
      },
      {
        "scopeName": "text.html.quarto.quartofn",
        "path": "./syntaxes/quartofn.tmLanguage.json",
        "injectTo": [
          "text.html.quarto"
        ]
      }
    ],
    "languages": [
      {
        "id": "quarto",
        "aliases": [
          "Quarto"
        ],
        "extensions": [
          ".qmd"
        ]
      }
    ],
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "quarto",
        "path": "./snippets/quarto.code-snippets"
      }
    ]
  },
  "extensionDependencies": [
    "quarto.quarto"
  ],
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/markdown-it": "^14.1.2",
    "@types/mocha": "^7.0.2",
    "@types/node": "20.x",
    "@types/vscode": "1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "glob": "^7.2.0",
    "mocha": "^10.8.2",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "vscode-test": "^1.3.0",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "markdown-it": "^14.1.0"
  }
}
