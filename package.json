{
  "name": "quarto-wingman",
  "displayName": "Quarto Wingman",
  "description": "",
  "version": "0.0.2",
  "publisher": "ntluong95",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ntluong95/quarto-wingsman.git"
  },
  "icon": "resources/icon.png",
  "bugs": {
    "url": "https://github.com/ntluong95/quarto-wingsman/issues"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "python",
    "r",
    "quarto"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "python.customInjection",
        "path": "./syntaxes/python-custom-injection.json",
        "injectTo": [
          "meta.embedded.block.python",
          "markup.fenced_code.block.markdown",
          "text.html.quarto",
          "text.html.markdown"
        ]
      },
      {
        "scopeName": "text.html.quarto.quartofn",
        "path": "./syntaxes/quartofn.tmLanguage.json",
        "injectTo": [
          "text.html.quarto"
        ]
      }
    ]
  },
  "dependencies": {}
}
